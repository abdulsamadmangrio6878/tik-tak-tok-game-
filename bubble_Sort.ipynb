   "source": [
    "def bubble(d):\n",
    "    s = len(d)\n",
    "    for f in range(s):\n",
    "        for i in range(s-f-1):\n",
    "            if d[i] > d[i+1]:\n",
    "                d[i],d[i+1] = d[i+1],d[i]\n",
    "    return d\n",
    "bubble([9,8,7,6,5,4,3,2,1])"
   ]
  }
